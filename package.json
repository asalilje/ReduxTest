{
  "name": "ReduxTest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:dev": "npm run clean-dist && concurrently \"npm run copy:dev\" \"npm run open:dev\" \"npm run lint:dev\" \"npm run test:dev\" \"npm run webpack:dev\"",
    "test": "mocha --spec progress ./tools/testSetup.js \"src/**/*.test.js\"",
    "test:dev": "npm test -- --watch",
    "build:prod": "npm run clean-dist && concurrently \"npm run copy:prod\" \"npm run lint\" \"npm run test\" \"npm run webpack:prod\" && npm run open:prod",
    "webpack:dev": "webpack-dev-server --config webpack.dev.config.js --inline --hot",
    "webpack:prod": "webpack --config webpack.prod.config.js",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "copy:dev": "babel-node ./tools/copyFiles.dev.js",
    "copy:prod": "babel-node ./tools/copyFiles.prod.js",
    "open:dev": "babel-node ./tools/openSite.dev.js",
    "open:prod": "babel-node ./tools/openSite.prod.js",
    "lint": "esw src tools",
    "lint:dev": "npm run lint -- --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lodash.throttle": "^4.1.1",
    "node-uuid": "^1.4.7",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.6.0",
    "eslint-watch": "^2.1.14",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "jsdom": "^9.8.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.1.2",
    "ncp": "^2.0.0",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.3.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
